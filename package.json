    "react-router": "^3.0.2",
    "react-sidebar": "^2.3.0",
    "react-toolbox": "^2.0.0-beta.7",
    "reactabular-table": "^8.6.0",
    "uuid": "^3.0.1",
    "webpack-hot-middleware": "^2.17.1"
  }
}
