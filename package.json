{
  "name": "essential-react",
  "version": "0.5.0",
  "description": "A minimal skeleton for building testable React apps using Babel",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "npm run build",
    "server": "node start.js",
    "build": "webpack -p --progress --config webpack.production.config.js",
    "test": "nyc ava --fail-fast --verbose --require babel-register",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pheuter/essential-react.git"
  },
  "keywords": [
    "skeleton",
    "template",
    "react",
    "quickstart"
  ],
  "author": "Mark Fayngersh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pheuter/essential-react/issues"
  },
  "homepage": "https://github.com/pheuter/essential-react",
  "devDependencies": {
    "autoprefixer": "^6.0.2",
    "ava": "^0.18.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.2",
    "coveralls": "^2.11.2",
    "css-loader": "^0.26.2",
    "enzyme": "^2.0.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "nyc": "^10.1.2",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.0.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "express": "^4.12.3",
    "fake-identity": "^1.0.6",
    "fixed-data-table": "^0.6.3",
    "phrase-generator": "^1.0.0",
    "random-datetime": "^1.0.1",
    "random-int": "^1.0.0",
    "random-item": "^1.0.0",
    "random-word": "^2.0.0",
    "react": "^15.0.1",
    "react-autocomplete": "^1.4.0",
    "react-bootstrap-table": "^3.1.0",
    "react-dom": "^15.0.1",
